					"render_method": "opaque" 
				},
				"north": {
		 			"texture": "side_cooking", 
					"render_method": "opaque" 
				},
				"west": {
		 			"texture": "side_cooking", 
					"render_method": "opaque" 
				},
				"east": {
		 			"texture": "side_cooking", 
					"render_method": "opaque" 
				}
			}
		}
	}
